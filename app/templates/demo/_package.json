{
  "name": "<%= projectName %>-demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for <%= projectName %>",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "<%= testingFramework === 'jest' ? 'jest' : 'ng test' %>",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm link <%= projectName %>"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "2.0.0",
    "@stackblitz/sdk":"^1.2.0",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26",
    "@angular/platform-server": "^6.0.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@angularclass/hmr": "~2.1.3",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",<% if(testingFramework === 'karma'){%>
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",<% } else if(testingFramework === 'jest') {%>
    "@types/jest": "22.2.3",
    "jest": "22.4.3",
    "jest-preset-angular": "5.2.1",<% } %>
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "ts-loader": "~4.2.0",
    "reflect-metadata": "~0.1.10",<% if(useCompodoc){%>
    "@compodoc/gulp-compodoc": "0.0.8",<% } %><% if(enforceNgGitCommitMsg){%>
    "commitplease": "3.2.0",<% } if(!skipGhReleasing) { %>
    "conventional-github-releaser": "2.0.2",<% } if(useYarn) { %>
    "greenkeeper-lockfile": "1.14.0",<% } if(!skipCoveralls) { %>
    "gulp-coveralls": "0.1.4",<% } if(!skipTravis) {%>
    "travis-status": "2.0.0",<% } %>
    "del": "3.0.0",
    "gulp": "3.9.1",
    "gulp-git": "2.6.0",
    "gulp-hub": "0.8.0",
    "gulp-bump": "3.1.1",
    "gulp-file": "0.4.0",
    "gulp-conventional-changelog": "1.1.24",
    "angular-cli-ghpages": "0.5.2",
    "run-sequence": "2.2.1",
    "lodash": "4.17.10",
    "yargs": "11.0.0",
    "ansi-colors": "2.0.1",
    "fancy-log": "1.3.2",
    "webpack-cli": "^2.0.14",
    "express": "^4.16.2",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0"
  }<% if(testingFramework === 'jest'){%>,
    "jest": {
      "preset": "jest-preset-angular",
      "coverageReporters": [ "lcov", "text" ],
      "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
    }<% } %>
}
